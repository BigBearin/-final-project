<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@drawable/white_back"
    tools:context="com.example.br161.finalproject.BackAndBicepsActivity">


    <!-- TextView for the Title of the page -->
    <TextView
        android:id="@+id/tvTitle"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:text="@string/Back_and_Bicep_Title"
        android:gravity="center_horizontal"
        android:textSize="30sp"
        android:textColor="@color/Black"/>

    <!-- TextView showing what the checkbox does on the right -->
    <TextView
        android:id="@+id/tvCBComplete"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Checkbox_Title"
        android:textColor="@color/Black"
        android:layout_below="@+id/tvTitle"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"/>

    <!-- TextView above the Workout column -->
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tvTitle"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:text="@string/Workout_Name_Title"
        android:textColor="@color/Black"/>

    <!-- End of the title portion of the XML -->


    <!-- TextView for the Overhand Grip Barbell Row Workout -->
    <TextView
        android:id="@+id/tvOGBRWorkout"
        android:paddingTop="4dp"
        android:layout_marginRight="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="30dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="15sp"
        android:textColor="@color/FireBrick"
        android:text="@string/Overhand_Grip_Barbell_Row_Workout"
        android:lines="1"
        android:layout_below="@+id/tvCBComplete"/>

    <!-- EditText for the weight for a workout -->
    <EditText
        android:id="@+id/tvWeight1"
        android:layout_toLeftOf="@+id/etReps1"
        android:layout_toStartOf="@+id/etReps1"
        android:layout_below="@id/tvCBComplete"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginRight="10dp"
        android:layout_marginEnd="10dp"
        android:textSize="15sp"
        android:hint="@string/Weight_Hint"
        android:textColorHint="@color/Black"
        android:textColor="@color/Black"/>

    <!-- EditText for the reps for a workout -->
    <EditText
        android:id="@+id/etReps1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:hint="@string/Reps_Hint"
        android:layout_below="@+id/tvCBComplete"
        android:layout_toLeftOf="@+id/cb1"
        android:layout_toStartOf="@+id/cb1"
        android:textSize="15sp"
        android:layout_marginRight="10dp"
        android:layout_marginEnd="10dp"
        android:textColor="@color/FireBrick"
        android:textColorHint="@color/FireBrick"/>

    <!-- Checkbox indicating that a workout was completed -->
    <CheckBox
        android:id="@+id/cb1"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tvCBComplete"/>

    <!--################## End of Workout 1 ######################-->


